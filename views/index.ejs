<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>App con Socket.io y EJS</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body class="container d-flex justify-content-center align-items-center">
  <main class="pt-5 pb-5 w-100">
    <!-- Productos -->
    <section class="border p-5 rounded mb-5">
      <div class="mb-5">
        <h1>Ingrese Producto</h1>
      </div>
      <div class="mb-4">
        <%- include('./partials/form.ejs'); %>
      </div>
      <div class="mb-4">
        <%- include('./partials/table.ejs'); %>
      </div>
    </section>
    <!-- Chat -->
    <section class="border p-5 rounded">
      <div class="mb-5">
        <h1>Centro de Mensajes</h1>
      </div>
      <div>
        <%- include('./partials/chat.ejs'); %>
      </div>
    </section>
  </main>
  <script src="/socket.io/socket.io.js"></script>
  <script src="/main.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</body>
</html>